{
  "expo": {
    "name": "use-nelaton-easily",
    "slug": "sergeitest",
    "version": "1.0.0",
    "owner": "zinaidamama",
    "scheme": "use-nelaton-easily",
    "orientation": "portrait",
    "icon": "./src/assets/images/splash-screen/appIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash-screen/SplashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#4BAAC5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "urocontrol.sergeikuleshov",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/splash-screen/SplashScreen.png",
        "backgroundColor": "#ffffff"
      },
      "package": "urocontrol.sergeikuleshov",
      "android.googleServicesFile": "./google-services.json",
      "permissions": [
        "SCHEDULE_EXACT_ALARM"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "fc73b2e7-9ffe-4f24-aff0-65b6aaebbc20"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fc73b2e7-9ffe-4f24-aff0-65b6aaebbc20"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./src/assets/images/splash-screen/notificationIcon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      "expo-localization",
      "expo-asset"
    ]
  }
}
